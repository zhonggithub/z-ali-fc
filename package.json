{
  "name": "z-ali-fc",
  "private": true,
  "version": "2.2.1",
  "main": "dist/index.js",
  "repository": "git@gitlab.com:hugozz/z-ali-fc.git",
  "author": "Zz <quitjie@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "lint": "eslint src/",
    "prebuild": "npm run lint && rm -rf dist/",
    "build": "babel --out-dir dist src --copy-files",
    "predeploy": "npm run lint && rm -rf code/",
    "deploy": "babel --out-dir code src --copy-files && cp -rf {package.json,.env,template.yml} code/ && cd code && npm install --production && fun deploy",
    "build-graphql": "babel-node src/buildGraphglSchema.js",
    "doc": "babel-node src/buildDoc.js && cd apidocs && mkdocs build",
    "view-doc": "cd apidocs && mkdocs serve",
    "test": "cross-env NODE_ENV=test nyc ava",
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node -- src/index.js | bunyan"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/node": "^7.10.3",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/polyfill": "^7.10.1",
    "@babel/preset-env": "^7.10.3",
    "@babel/register": "^7.10.3",
    "ava": "^3.9.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.21.2",
    "nodemon": "^2.0.4",
    "nyc": "^14.1.0"
  },
  "dependencies": {
    "@alicloud/fc2": "^2.2.1",
    "bluebird": "^3.7.2",
    "bunyan": "^1.8.14",
    "dotenv": "^8.2.0",
    "glob": "^7.1.6",
    "ioredis": "^4.17.3",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "mysql2": "^2.1.0",
    "seneca": "^3.22.0",
    "sequelize": "^6.3.3",
    "validator": "^13.7.0",
    "z-error": "^2.0.2",
    "zhz-util": "^3.0.4"
  },
  "ava": {
    "babel": true,
    "files": [
      "test/*.js"
    ]
  },
  "nyc": {
    "require": [
      "@babel/register",
      "@babel/polyfill"
    ],
    "include": [
      "src/"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
